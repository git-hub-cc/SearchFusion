{
  "manifest_version": 3,
  "name": "SearchFusion - 全能聚合搜索",
  "version": "2.3.0",
  "description": "聚合100+搜索引擎，支持AI、影视、学术等多领域搜索，自动解析与一键直达。",
  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  },
  "permissions": [
    "tabs",
    "scripting",
    "storage",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background/serviceWorker.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "utils/normalize.js",
        "engines/generic.js",
        "engines/parser/google.js",
        "engines/parser/bing.js",
        "engines/parser/baidu.js",
        "engines/parser/yandex.js",
        "engines/parser/toutiao.js",
        "engines/parser/liumingye.js",
        "engines/parser/gatherfind.js",
        "engines/parser/sbkko.js",
        "engines/parser/upyunso.js",
        "engines/parser/pansearch.js",
        "engines/parser/piratebay_1.js",
        "engines/parser/piratebay_2.js",
        "engines/parser/xcili.js",
        "engines/parser/rarbg.js",
        "engines/parser/quark_station.js",
        "engines/parser/xiaoyu.js",
        "engines/parser/bilibili.js",
        "engines/parser/imdb.js",
        "engines/parser/rottentomatoes.js",
        "engines/parser/cinematerial.js",
        "engines/parser/filmaffinity.js",
        "engines/parser/metacritic.js",
        "engines/parser/iyf.js",
        "engines/parser/nivod.js",
        "engines/parser/dsxys.js",
        "engines/parser/gimy.js",
        "engines/parser/gj5.js",
        "engines/parser/yueyu2.js",
        "engines/parser/acfun.js",
        "engines/parser/anime1.js",
        "engines/parser/xusou.js",
        "engines/parser/dmla8.js",
        "content-scripts/parser.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*",
        "engines/*"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}